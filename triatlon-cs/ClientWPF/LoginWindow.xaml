<Window x:Class="ClientWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientWPF"
        mc:Ignorable="d"
        Title="Login | Triatlon" Height="320" Width="250">
    <Grid>
        <Label Content="Triatlon" HorizontalAlignment="Left" Margin="20,31,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
        <Label Content="Login Arbitru" HorizontalAlignment="Left" Margin="20,55,0,0" VerticalAlignment="Top" FontSize="12"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="21,103,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Center" Margin="0,129,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" Name="EmailTextBox"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="21,152,0,0" VerticalAlignment="Top"/>
        <PasswordBox HorizontalAlignment="Center" Margin="0,178,0,0" VerticalAlignment="Top" Width="210" Name="PasswordTextBox"/>
        <Button x:Name="LoginButton" Content="Login" HorizontalAlignment="Left" Margin="20,230,0,0" VerticalAlignment="Top" Click="LoginButton_Clicked"/>
    </Grid>
</Window>
