        <font>
          <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <TableView fx:id="resultsTableView" layoutX="21.0" layoutY="53.0" prefHeight="324.0" prefWidth="557.0">
      <columns>
          <TableColumn fx:id="idColumn" editable="false" prefWidth="75.0" text="ID"/>
          <TableColumn fx:id="nameColumn" editable="false" prefWidth="347.0" text="Nume"/>
          <TableColumn fx:id="pointsColumn" editable="false" minWidth="0.0" prefWidth="134.0" text="Puncte"/>
      </columns>
    </TableView>
</AnchorPane>
